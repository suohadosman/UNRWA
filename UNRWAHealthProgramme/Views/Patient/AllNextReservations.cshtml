@model PatientViewDto
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/Patient/Dates.css" />
    <link href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css"
          rel="stylesheet" />
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css"
          integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
          crossorigin="anonymous" />

    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/patient/home .css" />
    <link href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css"
          rel="stylesheet" />
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css"
          integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
          crossorigin="anonymous" />
    <title>next dates</title>

</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light fixed-top" style="background-color: #1d86df; height: 3.5rem;">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon" style="font-size: 17px;"></span>
        </button>
        <div class="collapse navbar-collapse " id="navbarNavDropdown" style="background-color: #1d86df;">
            <ul class="navbar-nav" style="background-color: #1d86df;">

                <li class="nav-item active">
                    <a asp-action="Home" asp-controller="Patient" asp-route-personId="@Model.PersonIdToSearch" class="nav-link btn" id="logout-icon"
                       style="font-size: 17px; border: solid 1px; width: 132px; padding: 6px;text-align: center;">
                        Back
                    </a>

                </li>


            </ul>
        </div>
        <div class="nav__icons">
            <img src="~/images/unrwa-logo-0DB0E000FE-seeklogo.com.png" alt="logo" class=" img__1 img-fluid">
        </div>


    </nav>
    <div class="row">
        <div class="CardInner" style="margin: auto; margin-top: 100px ">
              @*  <label>search By Date Type: </label>
                <div class="box">
                    <div class="InputContainer">
                        <input placeholder="search" />
                    </div>

                    <button type="button" class="btn btn-primary Icon">
                        <i class="bx bx-search-alt-2"
                           style="color: #1d86df; font-size: 30px"></i>
                    </button>
                </div>*@
        </div>
    </div>
    <div class="container">
        <div class="row" style="    justify-content: space-between;">
            @foreach (var item in Model.AllNextReservations)
            {
                <div class="col-lg-4 col-md-12 col-sm-12">
                    <div class="card" style="margin-top: 10px">
                        <h3 style="color: #1d86df">@item.ServiceName</h3>

                        <div class="face face1 face_11">
                            <div class="content"></div>
                        </div>
                        <div class="face face2">
                            <div class="content"
                                 style="color: #1d86df !important; font-weight: 700">
                                <p>Center Name : @item.CenterName</p>
                                <p>Doctor Name : @item.DoctorName</p>
                                <p>Date :@item.Date.ToShortDateString()</p>
                                <p>Hour: @item.Date.ToShortTimeString()</p>
                            </div>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</body>
</html>
<!-- ALL JS FILES -->
<script src="~/js/jquery.min.js"></script>
<script src="~/js/popper.min.js"></script>
<script src="~/js/bootstrap.min.js"></script>
