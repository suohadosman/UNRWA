                    </div>
            <table id="example" class="table table-striped table-bordered">
                <thead>
                    <tr>

                        <th>Type</th>
                        <th>Name</th>
                        <th>Date</th>
                        <th>Result</th>
                        <th>Unit</th>
                    </tr>
                </thead>
                <tbody id="LabTestTable">
                    @if (Model.AllPatientLabTests.Count > 0)
                    {
                        @foreach (var item in Model.AllPatientLabTests)
                        {
                            <tr>


                                <td>@item.LabTestType</td>
                                <td>@item.LabTestName</td>
                                <td>@item.Date</td>
                                <td>@item.Result</td>
                                <td>@item.Unit</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
            <script src="~/js/browse the analytics.js"></script>
            <script src="~/js/Doctor/PatientLabTestFilter3.js"></script>

</body>
