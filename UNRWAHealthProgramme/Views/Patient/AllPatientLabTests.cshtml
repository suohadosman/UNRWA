                    <input type="hidden" value="@Model.PersonIdToSearch" id="personId" />
                    <label class="form-group">
                <h4 style="display:inline;">Filter By Lab Test Type</h4>

                        <select asp-for="LabTestTypeId" asp-items="@(new SelectList(Model.LabTestTypes,"Id","Type"))"
